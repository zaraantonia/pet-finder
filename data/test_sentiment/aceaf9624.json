  "sentences": [
    {
      "text": {
        "content": "Found a Siamese kitten behind my house in the jungle.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "Relatively healthy, with no visible wounds or sicknesses.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    },
    {
      "text": {
        "content": "Very affectionate, craves attention and currently needs a home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "If you have lost this kitten around my area, or if you know someone who has, kindly inform me via SMS.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "If you would like to adopt it, kindly hold on for about 3 weeks after this post, so the owner can claim him if they want to.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": -0.1
      }
    }
