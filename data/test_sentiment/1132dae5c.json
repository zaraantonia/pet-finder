{
  "sentences": [
    {
      "text": {
        "content": "This dog was found at the junction of YMCA and Island Hospital, Penang.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "He is estimated to be about 4 to 6 years of age and had only a tick collar on him with no further means of identification.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "No microchip.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "He was almost hit by on coming traffic but a good samaritan brought him to safety.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "If this is your dog please contact GrASS Penang.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "This dog will be put up for adoption 7 days after this notice is issued if the owner doesn't claim the dog.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    }
  ],
