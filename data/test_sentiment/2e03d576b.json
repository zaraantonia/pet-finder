{
  "sentences": [
    {
      "text": {
        "content": "These kittens were left outside our gate 2 months ago.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "They were too young then, thus we decided to take care of them until they are old enough to be a adopted.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "They are 3 months old now and very active.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "All are healthy and very affordable.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "We would like them to go to good homes as they deserve one.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    },
    {
      "text": {
        "content": "We are not able to care for them anymore as we also have grown up cats of our own.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": -0.6
      }
    },
    {
      "text": {
        "content": "Please adopt.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
