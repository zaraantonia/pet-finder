{
  "sentences": [
    {
      "text": {
        "content": "Minie and her 2 sisters were being abandoned in a box in front of a clinic at Seri Kembangan, Selangor.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    },
    {
      "text": {
        "content": "About 1.5kg weight, healthy baby, no ticks/fleas.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Her tail had been cut off when found.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Please contact Mun or Mrs Tang if you can give her a loving home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    },
    {
      "text": {
        "content": "Adopter is required to reimburse vaccination fee.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
