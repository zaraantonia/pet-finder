    {
      "text": {
        "content": "Location: Pulau Tikus, Penang Esther is 5 weeks old.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "She was a lost baby.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "The rescuer is unable to keep her,because she is only renting only a room in a apartment and it's not big enough for a cat.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": -0.7
      }
    },
    {
      "text": {
        "content": "She'd like to find Esther a nice home with space to run around and people who loves cats.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "Esther visited a vet at GIl.s veterinary and got dewormed,defleed so she is clean and healthy girl.She is waiting for a new home...thank you!",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    }
