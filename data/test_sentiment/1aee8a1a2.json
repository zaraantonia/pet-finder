  "sentences": [
    {
      "text": {
        "content": "Duke is a gorgeous young male pup caught on the the University of Nottingham Malaysia Campus.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "A big healthy boy, he is very laid-back and confident, and looks like he is going to be medium-large in size - the same as his Mum.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "He was the strongest in his litter, loves his food, and will make a great guard dog for someone who gives him a home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "He has received his first vaccination and has been dewormed and deflead.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
  ],
