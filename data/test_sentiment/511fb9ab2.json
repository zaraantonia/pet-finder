  "sentences": [
    {
      "text": {
        "content": "Three in a litter Male - brown; alert &amp; intelligent eyes; floppy ears; quiet and gentle; likes to be petted and belly rubbed.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    },
    {
      "text": {
        "content": "Female A - brown; beagle eyes; active; vocal in the presence of strangers; loves to play catch and enjoys belly rubs Female B - brown; gentle eyes; wide grin; active but fractured left hind leg in an accident; manages well with a limp.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "All three need exercise daily and/or space for play.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "All housetrained.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "The person who found this litter couldn't find a suitable home for them and have asked me to try the internet rather than just put them to sleep.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": -0.6
      }
    }
