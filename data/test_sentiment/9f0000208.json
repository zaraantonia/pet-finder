  "sentences": [
    {
      "text": {
        "content": "Seeking someone can really give them a home that full of love,treat them like your own precious family members.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "These cutie will turn into medium size for next few months,NOT SUITABLE for apartment residence or small area,please consider before adopt them.Thank you.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    },
    {
      "text": {
        "content": "Adopter should agree to do so...",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "*Provide balanced meals and fresh water all the time.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "*Must be neutered when they're about 6 months old.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "*Remember to bring them for their yearly vaccination.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    },
    {
      "text": {
        "content": "*Allow for home visit once in a month.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Do Whatsapp me if you are interested.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "Location:Johor Bahru ONLY SERIOUS ADOPTERS WILL BE ENTERTAINED.",
        "beginOffset": -1
