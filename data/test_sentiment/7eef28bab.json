  "sentences": [
    {
      "text": {
        "content": "Ginger is the second-largest and most dominant pup in a part-Ridgeback litter of strays on the University of Nottingham Malaysia Campus.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "We have been feeding her and her litter mates and slowly adopting them out.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Her adopter will need to agree to neuter her once she is old enough.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "We will take and hold the RM fee which will be used to pay for operation when she is old enough.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": -0.1
      }
    }
  ],
