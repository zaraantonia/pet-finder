{
  "sentences": [
    {
      "text": {
        "content": "Rescued him before CNY .",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "Underfed, diarrhoea and was bullied by other cats.",
