  "sentences": [
    {
      "text": {
        "content": "Found this tricolor beagle, age about 2 years old nearby my residential area.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Have tried various method to search the owner but with no success.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    },
    {
      "text": {
        "content": "Decided to let him go for adoption.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "I am staying in an apartment and already owned a beagle (5 years old), cant afford to own another due to limited space.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    },
    {
      "text": {
        "content": "This is my 2nd week taking care of him, so far he can eat &amp; sleep well.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "Mix well with my old dog as well, and very playful too.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    }
