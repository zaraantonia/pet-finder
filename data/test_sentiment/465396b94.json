  "sentences": [
    {
      "text": {
        "content": "Missy was dumped at our front gate we suspect by the rubber tappers nearby on 10 October .",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "She is a lovely puppy and is affectionate, intelligent, comes when called, sweet natured.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "She will be spayed by us when the time comes, if not yet adopted and we are willing to pay for her medical fees.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "Lovely colouring and unusual hazel - green eyes.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "She looks like a beagle or a mini St Bernard.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "A perfect healthy puppy looking for a loving home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    }
