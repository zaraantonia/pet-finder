  "sentences": [
    {
      "text": {
        "content": "I rescued Pat and her 3 brothers when they were about 2 months old.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Her 3 brothers have been adopted.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Her mother was spayed after her 4 kitties had been weaned off milk, and she was returned to the place where I found them.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "Pat is very adorable, playful, manja and enjoys being cuddled.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "She loves sleeping in a hammock fixed inside a cage and has a cute bobtail.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "She has been spayed and is ready to be adopted by a loving family.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    }
